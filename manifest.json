{
  "name": "Old Twitter",
  "description": "Long live the bird",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "/assets/icon16.png",
    "32": "/assets/icon32.png",
    "48": "/assets/icon48.png",
    "128": "/assets/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/assets/icon16.png",
        "/assets/icon32.png",
        "/assets/icon48.png",
        "/assets/icon128.png"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ]
}